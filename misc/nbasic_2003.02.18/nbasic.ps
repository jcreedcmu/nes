% 
% Ideally, before we can draw a line of text, we need to collect all the
% words that will be on it, just as I do in my Beest HTML viewer, as well
% as character attributes for each word. But for now, this implementation 
% does not bother. It determines the maximize ascent and descent after
% drawing the text, not before. XX
% 
% ----------- Functions ------------
/updateFont { 
	/f0 null def 
	(Times) fontFamily eq (Times New Roman) fontFamily eq or { 
		bold { 
			italic { /TBoldItalicISO } { /TBoldISO } ifelse 
		} { 
			italic { /TItalicISO } { /TRomanISO } ifelse 
		} 
		ifelse 
	} if 
	(Helvetica) fontFamily eq (Arial) fontFamily eq or { 
		bold { 
			italic { /HBoldItalicISO } { /HBoldISO } ifelse 
		} { 
			italic { /HItalicISO } { /HRomanISO } ifelse 
		} 
		ifelse 
	} if 
	(Courier) fontFamily eq (Courier New) fontFamily eq or { 
		bold { 
			italic { /CBoldItalicISO } { /CBoldISO } ifelse 
		} { 
			italic { /CItalicISO } { /CRomanISO } ifelse 
		} 
		ifelse 
	} if 
	(Symbol) fontFamily eq { 
		bold { 
			italic { /SBoldItalicISO } { /SBoldISO } ifelse 
		} { 
			italic { /SItalicISO } { /SRomanISO } ifelse 
		} 
		ifelse 
	} if 

	findfont /f0 exch def  
	/bboxBottom f0 /FontBBox get 1 get 1000 div fontSize mul -1 mul def 
	/bboxTop    f0 /FontBBox get 3 get 1000 div fontSize mul def 
	f0 fontSize scalefont setfont 
	lineAscent bboxTop lt { /lineAscent bboxTop def } if  
	lineDescent bboxBottom lt { /lineDescent bboxBottom def } if  
	/fontAscent bboxTop def 
	/fontDescent bboxBottom def 
} def
/FS { 
	/fontSize exch def updateFont 
} def 
/F { 
	/fontFamily exch def updateFont 
} def 
/resetX { 
	/x leftMargin def
} def 
/resetY { 
	/y pageHeight topMargin sub def 
} def 
/BR {  
	/oldx x def 
	/y y lineAscent lineDescent add sub def  
	resetX 
	y bottomMargin lt { 
		showpage 
		/didShowPage true 
		resetY 
	} if 
	oldx 0 eq didBR and { /didParSkip true def } if 
	/didBR true def 
	% /lineAscent 0 def 
	% /lineDescent 0 def 
} def 
/P { 
	didParSkip not { BR } if 
	didParSkip not { BR } if 
} 
def 
/acharpath { 
	/acstr exch def pop /acsp exch def 
	newpath  
	str { 
		/ch exch def 
		1 string 0 ch put false charpath 
		acsp 0 rmoveto 
	} forall 
} def 
/A { 
	/str exch def 
	/w str stringwidth pop 
		str length intercharSpace mul add 
		def 
		x w add rightLimit ge { BR } if 
	x y moveto 
	outline {                                           
		shadow {                                    
			1 -0.1 0 {                        
				/offset exch def 
				offset setgray                 
				x offset 3 mul add y offset 3 mul sub moveto 
				intercharSpace 0 str acharpath 
				% str false charpath 
				fontSize 30 div setlinewidth stroke 
			} for                               
			0 setgray 
		} {                                         
			intercharSpace 0 str acharpath      
			% str false charpath 
			fontSize 30 div setlinewidth stroke 
		} ifelse                                    
	} {                                                 
		shadow {                                    
			1 -0.1 0 {                        
				/offset exch def 
				offset setgray                 
				x offset 3 mul add y offset 3 mul sub moveto 
				intercharSpace 0 str ashow  
				% str show 
			} for                               
			0 setgray 
		} {                                         
			intercharSpace 0 str ashow          
			% str show 
		} ifelse                                    
	} ifelse                                            
	strike {							
		newpath fontSize 20 div setlinewidth			
		x y fontAscent 0.32 mul add dup /y2 exch def moveto	
		x w add y2 lineto stroke				
	} if								
	underline {							
		newpath fontSize 20 div setlinewidth			
		x y fontAscent 0.2 mul sub dup /y2 exch def moveto	
		x w add y2 lineto stroke				
	} if								
	overline {							
		% I don't think RTF supports this, but it can be used later. 
		newpath fontSize 20 div setlinewidth			
		x y fontAscent 1.2 mul add dup /y2 exch def moveto	
		x w add y2 lineto stroke				
	} if					
	/x x w add def  			
	/didBR false def 			
	/didShowPage false def 			
} def 

% These are only binary for now 
